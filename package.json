{
  "name": "nightwatchjs-basic",
  "version": "1.1.0",
  "description": "helps to get started with nightwatchjs setup and test run on web browsers.",
  "main": "index.js",
  "scripts": {
    "test-e2e": "nightwatch -c nightwatch.conf.js",
    "test:desktopChrome": "nightwatch -c nightwatch.conf.js ./tests/web.js --tag desktopChrome -e desktopChrome",
    "test:androidChrome": "nightwatch -c nightwatch.conf.js ./tests/web.js --tag androidChrome -e androidChrome",
    "test:androidNative": "nightwatch -c nightwatch.conf.js ./tests/app.js --tag androidNative -e androidNative"
  },
  "repository": {
    "type": "git",
    "url": "git+https://jagadeeshshetty@github.com/jagadeeshshetty/nightwatchjs-basic.git"
  },
  "keywords": [
    "nightwatchjs",
    "javascript",
    "selenium",
    "webdriver",
    "test",
    "automation",
    "chrome"
  ],
  "author": "JAGADEESH C",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jagadeeshshetty/nightwatchjs-basic/issues"
  },
  "homepage": "https://github.com/jagadeeshshetty/nightwatchjs-basic#readme",
  "dependencies": {
    "nightwatch": "^1.1.11",
    "chromedriver": "^83.0"
  }
}
